 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */


@import "compass";
@import "partials/html5-boilerplate";
@import "partials/_variables";
@import "partials/vanilla-overrides";

/* apply a natural box layout model to all elements */
* { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }






/* Lydmaskinen V2 */

/* Basics */

body {
	font-family: Arial, sans-serif;
	background-color: #ebeef1;
}

a, a:active, a:visited {
    color: $blue;
    text-decoration: none;
    @include transition-property(color);
    @include transition-duration(200ms);

    &:hover {
        color: darken($blue, 20%);
    }
}

.bubble{
    color: white;
    margin: 0 3px;
    padding: 2px 6px;
    font-weight: 600;
    background-color: gray;
    @include border-radius(12px);
    @include box-shadow(
        white 0 0 0 2px,
        rgba(0,0,0,0.4) -1px 2px 3px 0
    );
    @include transition-duration(200ms);
    opacity: 0.5;

    &.red{
        @include bubble-gradient(red);
    }
    &.blue{
        @include bubble-gradient($blue);
    }
    &.green{
        @include bubble-gradient(#2a8923);
    }
    &:hover {
        opacity: 1;
    }
}

a.Button {
    //extends basic Vanilla button
    display: inline-block;
    margin-bottom: 8px;
}



/* Typography */

h3,
div.Box h4 {
    color: #202020;
    font-family: "helvetica neue", helvetica, arial, sans-serif;
    font-size: 17px;
    font-weight: 400;
    text-transform: uppercase;
    margin-bottom: 1em;
}

#title, h2, h3 {
	font-weight: normal;
}

#title > a {
	display: block;
	text-indent: -9999px;
	height: 72px;
	width: 310px;
	background: transparent url('../images/logo.png') no-repeat -2px -1px;
}





/* Layout */

.wrapper{
	margin:auto;
	width:$masterwidth;
}
#main.wrapper {
	overflow: visible;
}

.leftcol {
	@include leftcol;
}

.rightcol {
	@include rightcol;
}

.stayright{
	position: absolute;
	right: 12px;
}






/* Header */

#header-container{
	background: transparent url('../images/body_bg.jpg') repeat-x 0 top;
	height:326px;
	margin-bottom: 36px;

    .toolbar{
    	position: relative;
    	top: 13px;

        a{
            margin: 0 4px;
            text-decoration: none;
            font-weight: 200;

            &:first-child{
                margin: 0 3px 0 0;
            }

            &:hover .bubble{
                opacity: 1;
            }
        }
    }
}

#searchbox {
	position: relative;
	top: 7px;
	clear: left;
	background-color: white;
    border-radius: 6px;
    border: 1px solid #e1e1e1;
	height: 25px;

    input[type='text'] {
        font-size: 14px;
        font-weight: 200;
    	border: none;
    	position: relative;
    	top: 5px;
    	width: 260px;
    }

    input[type='submit'] {
        background: transparent url('../images/looking-glass.png') no-repeat 13px 5px;
        float: left;
        border: none;
        height: 20px;
        width: 33px;
        text-indent: -9999px;
        opacity: 0.6;
        @include transition-property(opacity);
        @include transition-duration(200ms);
    }

    &:hover input[type='submit'] {
        opacity: 1;
    }
}



nav{
	position: relative;
	top: 28px;

    li {
    	margin-left: 28px;
    	float: left;
        &:first-child {
            margin-left: 0;
        }

        a{
        	display: block;
        	position: relative;
        	text-indent: -9999px;
        	background: transparent url('../images/nav.png') no-repeat 0 3px;
        	padding: 6px 0;
            margin-right: 6px;
            top: -3px;
            border-width: 3px 0;
            border-style: solid;
            border-color: rgba(255,0,0,0);
            @include transition-property(border);
            @include transition-duration(200ms);
            &:hover,
            &:active,
            &.active {
                border-color: rgba(255,0,0,1);

            }
        }
    }
    #forum{
    	width: 78px;
    }
    #news{
    	width: 102px;
    	background-position: -112px 3px;
    }
    #marketplace{
    	width: 93px;
    	background-position: -248px 3px;
    }
    #tipsandtricks{
    	width: 157px;
    	background-position: -375px 3px;
    }
}






/* Forum */

#main article {
    section {
        @include rounded-box;
        display: block;
        width: 642px;

        .post {
            .left {
                float: left;
                margin: 0 1em 1em 0;
            }
            .right {
            	float: right;
            	margin: 0 0 1em 1em;
            }
            p {
            	margin-bottom: 1em;
            }
            .attachment,
            .link {
            	font-size: 12px;
            }
        }

        .toolbar {
        	position: relative;
            height: 30px;
        	padding: 7px 17px;
        	border-color: #e1e1e1;
        	border-style: solid;
        	border-width: 0;
            &.top {
                border-bottom-width: 1px;
            }
            &.bottom {
                border-top-width: 1px;
            }

            a {
                margin: 3px;
                &:first-child{
                    margin-left: 0;
                }
                &.like img {
                 	margin-right: 7px;
                }
            }
        }

        &.announcement .post {
            font-size: 14px;
        }

        &.headline h2 {
            @include blue-header;
        }
    }

    #newPost {
        font-size: 16px;
    }
}

ul.iconlist {
	@include nobullets;

    padding: 16px;

    li {
        margin-top: 16px;
    	overflow: auto;
        &:first-child {
         	margin-top: 0;
        }

        a {
        	text-decoration: none;
        }
    }

    .iconlist-icon,
    .iconlist-content {
        float: left;
    }
    .iconlist-icon {
        margin-right: 16px;
        border: 1px solid #e1e1e1;
    }
}

ul.DataList {
    @include nobullets;

    li.Item {
        padding: 0 0 6px 0;

        .ItemContent {
            margin: 0;
        }

        .Options,
        .MItem.Category {
            display: none;
        }
    }
}



.MessageList {

    // Common styles
    .Item {
        .Meta {
            height: 63px;
            @include background-image(linear-gradient(top,  white 1%, #f7f7f7 100%));
        }

        .Message {
            padding: 16px;
        }
    }

    // The initial post
    &.Discussion {
        .Item {
            height: 294px; // TODO: Remove this rule!
        }
    }

    // The answer thread
    li.Item {
        @include rounded-box;
        .Meta {
            @include border-radius(6px 6px 0 0);
        }
    }

}




/* Sidebar */

#main aside {
	float: left;
	margin-right: 40px; //39 on the front page

    section {
        @include rounded-box;
        width: 274px;
        background: #ffffff; /* Old browsers */
        background: -moz-linear-gradient(top, #ffffff 0%, #f7f7f7 100%); /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#f7f7f7)); /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top, #ffffff 0%,#f7f7f7 100%); /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top, #ffffff 0%,#f7f7f7 100%); /* Opera11.10+ */
        background: -ms-linear-gradient(top, #ffffff 0%,#f7f7f7 100%); /* IE10+ */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f7f7f7',GradientType=0 ); /* IE6-9 */
        background: linear-gradient(top, #ffffff 0%,#f7f7f7 100%); /* W3C */

        h3 {
        	font-weight: bold;
        }
    }

    .subsection {
    	margin-bottom: 3em;

        ul.iconlist li .iconlist-content {
        	width: 200px;
        }
    }

    .banner{
    	margin-bottom: 15px;
    }
}



/* Banners */

.banner{
	display: block;
}

#topbanner{
	text-align: center;
	margin-bottom: 35px;
}




/* Profile */

body.Profile {

    .ProfilePhotoLarge {
        @include border-radius(6px);
    }

    #Content {
        margin: 0;
        @include rightcol;

        .Profile {
            @include rounded-box;

            .User {
                padding: 16px;
                line-height: 2em;

                h1 {
                    font-size: 16px;
                }

                .About {

                    dl {
                        width: 100%;
                        overflow: auto;

                        dt, dd {
                            float: left;
                            width: 50%;
                        }
                        dt {
                            font-weight: bold;
                        }
                    }
                }
            }

            .Tabs {

            }

            .TabContent {
                padding: 16px;
            }
        }
    }
}



/* Other */

form {
    ul {
        @include nobullets;
    }
}

form.poll {
	font-size: 14px;
	line-height: 2em;

    input{
    	margin-right: 1em;
    }
}

#Popup {
    h1 {
        @include blue-header;
    }
}


#stats {
	color: #404040;
}

div.MessageForm
{
    @include rounded-box;
}

#main article > .banner,
#main article > input {
	margin-bottom: 15px;
}

#whosonline {
    .member {}
    .donator {color:#006600}
    .moderator {color:#cc3300}
}

footer {
	margin: 3em 0;
}

footer a,
footer span {
	margin: 0 0.5em 0 0.5em;
}












.ir { display: block; text-indent: -999em; overflow: hidden; background-repeat: no-repeat; text-align: left; direction: ltr; }
.hidden { display: none; visibility: hidden; }
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }
.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus { clip: auto; height: auto; margin: 0; overflow: visible; position: static; width: auto; }
.invisible { visibility: hidden; }
.clearfix:before, .clearfix:after { content: "\0020"; display: block; height: 0; overflow: hidden; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }


@media all and (orientation:portrait) {

}

@media all and (orientation:landscape) {

}

@media screen and (max-device-width: 480px) {

  /* html { -webkit-text-size-adjust:none; -ms-text-size-adjust:none; } */
}


@media print {
  * { background: transparent !important; color: black !important; text-shadow: none !important; filter:none !important;
  -ms-filter: none !important; }
  a, a:visited { color: #444 !important; text-decoration: underline; }
  a[href]:after { content: " (" attr(href) ")"; }
  abbr[title]:after { content: " (" attr(title) ")"; }
  .ir a:after, a[href^="javascript:"]:after, a[href^="#"]:after { content: ""; }
  pre, blockquote { border: 1px solid #999; page-break-inside: avoid; }
  thead { display: table-header-group; }
  tr, img { page-break-inside: avoid; }

@page { margin: 0.5cm; }
  p, h2, h3 { orphans: 3; widows: 3; }
  h2, h3{ page-break-after: avoid; }
